{
  "name": "@conversabi/sdk",
  "version": "1.0.0",
  "description": "Open source SDK for ConversaBI - AI-native business intelligence platform",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "demo": "cd demo && npm run dev",
    "docs": "typedoc src/index.ts",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "conversational-ai",
    "business-intelligence",
    "mcp",
    "sdk",
    "typescript",
    "react"
  ],
  "author": "ConversaBI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/conversabi/javascript-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/conversabi/javascript-sdk/issues"
  },
  "homepage": "https://docs.conversabi.dev",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/events": "^3.0.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "rollup": "^4.5.0",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "events": "^3.3.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
